<%- include('includes/head.ejs') %>
<% if (users.length !== 0) { %>
<h3>List of Users:</h3>
<ul class="user-list">
    <% for (let user of users) { %>
        <li><%= user %></li>
    <% } %>
</ul>
<% } else { %>
    <h3>No Users have been recorded.<br>Please head <a href="/">here</a> to add more users.</h3>
<% } %>


<%- include('includes/end.ejs') %>
